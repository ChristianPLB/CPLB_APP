{
  "expo": {
    "name": "LyriX",
    "slug": "LyriX",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/lyrix.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/lyrix.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.christian09232004.Lyri"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3e1832a1-e237-4097-8036-873729058df3"
      }
    }
  }
}
